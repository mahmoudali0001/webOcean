<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.app.ocean.Login</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddNewUser</servlet-name>
        <servlet-class>com.app.ocean.AddNewUser</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AddNewUser</servlet-name>
        <url-pattern>/AddNewUser</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>AddNewEmp</servlet-name>
        <servlet-class>com.app.ocean.AddNewEmp</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AddNewEmp</servlet-name>
        <url-pattern>/AddNewEmp</url-pattern>
    </servlet-mapping>

</web-app>